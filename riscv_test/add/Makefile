build_add_cycle:
	riscv64-unknown-linux-gnu-g++  -march=rv64gcv -mabi=lp64d -O3 add_cycle.cpp  -o add_cycle  -static  
run_add_cycle:
	/home/pp/Desktop/riscv/rv-v/riscv-gnu-toolchain/linux/bin/spike --isa=rv64gcv /home/pp/Desktop/riscv/rv-v/riscv-gnu-toolchain/linux/bin/pk add_cycle

